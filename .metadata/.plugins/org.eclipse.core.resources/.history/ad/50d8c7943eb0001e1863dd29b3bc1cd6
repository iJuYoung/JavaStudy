package casting;

public class MainClass1 {

	public static void main(String[] args) {
		Parent p = new Child();
		
		p.method1();//parent에만 정의된 메소드
		p.method2();//오버라이딩 된 메소드는 상관없이 인스턴스 내 자식 쪽을 호출
		p.method3();//p 타입 변수로는 자식 쪽에만 정의된 요소는 호출 불가능. 
		System.out.println("-------------------------------------");
		
		
		

	}

}
